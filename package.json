{
  "name": "taskmanager",
  "version": "0.0.1",
  "description": "Task manager web application",
  "main": "dist/index.js",
  "bin": {
    "taskmgr": "dist/bin/taskManager.js"
  },
  "engines": {
    "node": "8"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "./dist/",
      "./migrations/",
      "./seeds/",
      "./client/",
      "./node_modules/",
      "./src/bin",
      "./public/assets",
      "webpack.config.babel.js",
      "gulpfile.babel.js",
      "./coverage"
    ]
  },
  "scripts": {
    "test": "jest",
    "prepareDb": "npm run sequelize db:migrate:undo:all && npm run sequelize db:migrate",
    "babel-node": "babel-node",
    "eslint": "eslint",
    "gulp": "gulp",
    "build": "npm run webpack -- -p --env production && babel src --out-dir dist --source-map inline",
    "start": "gulp server",
    "sequelize": "sequelize",
    "nodemon": "nodemon",
    "webpack": "webpack",
    "watch": "webpack --watch",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VladVes/project-lvl4-s131.git"
  },
  "keywords": [
    "Task",
    "manager"
  ],
  "author": "Vladimir Veselov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VladVes/project-lvl4-s131/issues"
  },
  "homepage": "https://github.com/VladVes/project-lvl4-s131#readme",
  "dependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-polyfill": "*",
    "babel-preset-env": "*",
    "babel-preset-flow": "*",
    "babel-preset-stage-0": "*",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.0.0-beta",
    "css-loader": "^0.28.7",
    "debug": "*",
    "dotenv": "^4.0.0",
    "gulp": "^4.0.2",
    "jquery": "^3.2.1",
    "jquery-ujs": "^1.2.2",
    "keygrip": "^1.0.2",
    "koa": "*",
    "koa-bodyparser": "*",
    "koa-flash-simple": "*",
    "koa-generic-session": "*",
    "koa-logger": "*",
    "koa-methodoverride": "*",
    "koa-pug": "3.0.0-2",
    "koa-router": "next",
    "koa-static": "*",
    "koa-webpack": "*",
    "lodash": "^4.17.4",
    "moment": "^2.19.4",
    "mysql2": "^1.5.1",
    "pg": "^7.4.0",
    "popper": "^1.0.1",
    "popper.js": "^1.12.9",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "precss": "^2.0.0",
    "rollbar": "^2.3.2",
    "sequelize": "*",
    "sequelize-cli": "*",
    "sqlite3": "^3.1.13",
    "style-loader": "^0.19.1",
    "sugarcss": "^1.0.3",
    "sugarss": "^1.0.1",
    "umzug": "^2.1.0",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-eslint": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-flowtype": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jest": "*",
    "jest": "*",
    "jest-supertest-matchers": "*",
    "nodemon": "*",
    "supertest": "*",
    "nock": "^9.1.0"
  }
}
